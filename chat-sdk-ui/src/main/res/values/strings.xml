<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!--
      General UI actions and buttons
      These are used for common dialog actions and confirmations throughout the chat UI.
    -->
    <!-- Button label for confirming an action in dialogs and popups -->
    <string name="ok">OK</string>
    <!-- Button label for cancelling an action in dialogs and popups -->
    <string name="cancel">Cancel</string>
    <!-- Button label for completing an action, e.g. form submission -->
    <string name="done">Done</string>
    <!-- Button to open custom fields editor in pre-chat or thread details -->
    <string name="change_details_label">Change custom fields</string>
    <!-- Action to set or edit the conversation/thread name -->
    <string name="change_thread_name">Set conversation name</string>
    <!-- Button label for sending a chat message -->
    <string name="text_send">Send</string>
    <!-- Label indicating an option was selected (e.g. quick reply, list picker) -->
    <string name="action_was_selected">Selected</string>
    <!-- Button label for selecting an item from a list or picker -->
    <string name="select">Select</string>
    <!-- Button label for confirming a selection or action -->
    <string name="confirm">Confirm</string>
    <!-- Button label for selecting all items in a list -->
    <string name="select_all">All</string>
    <!-- Button label for deselecting all items in a list -->
    <string name="select_none">None</string>
    <!-- Action to select items in a list -->
    <string name="select_items">Select items</string>

    <!--
      Thread and conversation management
    -->
    <!-- Labels for thread/conversation state in the conversation list -->
    <string-array name="thread_state_names">
        <item>Current</item>
        <item>Archived</item>
    </string-array>
    <!-- Title for the conversation/thread list screen -->
    <string name="thread_list_title">Conversations</string>
    <!-- Content description for archive conversation button -->
    <string name="archive_thread_content_description">Archive conversation</string>
    <!-- Label for thread/conversation name field -->
    <string name="label_thread_name">Conversation name</string>
    <!-- Banner or label for archived threads -->
    <string name="label_thread_archived">This conversation was archived</string>
    <!-- Label for closed live chat thread -->
    <string name="label_livechat_thread_archived">This conversation was closed.</string>
    <!-- Empty state label for thread list -->
    <string name="label_no_threads">No conversations</string>

    <!--
      Chat state and error actions
    -->
    <!-- Button label for closing the chat -->
    <string name="close_chat">Close chat</string>
    <!-- Action label for closing chat in offline state (references close_chat) -->
    <string name="chat_offline_action_close">@string/close_chat</string>
    <!-- Button label for closing the chat during loading state -->
    <string name="loading_close_button_text">Close the Chat</string>

    <!--
      Chat message input and status
    -->
    <!-- Hint in the message input field -->
    <string name="hint_enter_a_message">Type a message…</string>
    <!-- Typing indicator for agent -->
    <string name="text_agent_typing">Agent is typing…</string>
    <!-- Loading more messages indicator in chat -->
    <string name="text_loading_more_messages">Loading</string>
    <!-- Message for unsupported message types in chat -->
    <string name="text_unsupported_message_type">Unsupported message type</string>
    <!-- Content description for text input field (accessibility) -->
    <string name="content_description_text_input">Text input</string>
    <!-- Title for image preview screen -->
    <string name="image_preview_title">Image preview</string>
    <!-- Title for video preview screen -->
    <string name="video_preview_title">Video preview</string>
    <!-- Message shown while SDK is initializing -->
    <string name="preparing_sdk">Preparing chat</string>
    <!-- Message read status label -->
    <string name="status_read">Read</string>
    <!-- Message sent status label -->
    <string name="status_sent">Sent</string>
    <!-- Message delivered status label -->
    <string name="status_delivered">Delivered</string>
    <!-- Message sending status label -->
    <string name="status_sending">Sending</string>
    <!-- Message failed status label -->
    <string name="status_failed">Failed</string>
    <!-- General preparing indicator -->
    <string name="preparing">Preparing</string>
    <!-- General loading indicator -->
    <string name="loading">Loading…</string>
    <!-- Message for delayed operations -->
    <string name="loading_delay_message">The operation is taking longer than expected.</string>
    <!-- Offline status label -->
    <string name="offline">Offline</string>
    <!-- Banner for offline state -->
    <string name="offline_banner">We are currently offline.</string>
    <!-- Message for offline state -->
    <string name="offline_message">Please check back soon.</string>
    <!-- User is next in queue label -->
    <string name="position_in_queue_next">You are next in line.</string>
    <!-- User's position in queue label -->
    <string name="position_in_queue_cardinal">You are number <xliff:g example="5" id="counter">%1$d</xliff:g> in line.</string>
    <!-- Supporting text for queue position card, one line shouldn't be wider than 30 characters. -->
    <string name="position_in_queue_supporting_text">Please hold, we\'ll be with you shortly.\nYour patience is appreciated!</string>

    <!--
      Conversation end and livechat
    -->
    <!-- Button label for ending a conversation -->
    <string name="action_end_conversation">End conversation</string>
    <!-- Message shown when conversation is closed with agent name -->
    <string name="livechat_conversation_closed_message">You chatted with</string>
    <!-- Message shown when conversation is closed without agent -->
    <string name="livechat_conversation_closed_message_no_agent">This conversation was closed.</string>
    <!-- Button to start a new chat -->
    <string name="livechat_new_chat">Start a new chat</string>
    <!-- Button to return to conversation transcript -->
    <string name="livechat_show_transcript">Back to conversation</string>
    <!-- Button to close live chat -->
    <string name="livechat_close_chat">Close chat</string>
    <!-- Options menu for conversation -->
    <string name="livechat_conversation_options">Conversation options</string>

    <!--
      Document and attachment support
    -->
    <!-- Dialog title for unsupported document type error -->
    <string name="unsupported_type_title">Unsupported document type</string>
    <!-- Error message when user tries to open unsupported document type -->
    <string name="unsupported_type_message">You do not have a viewer for documents of type %1$s installed. Visit the google play store for options.</string>
    <!-- Title for attachment picker dialog -->
    <string name="title_attachment_picker">Select attachment</string>
    <!-- Title for attachments screen or section -->
    <string name="attachments_title">Attachments</string>
    <!-- Label for extra attachments count (e.g. +3) -->
    <string name="extra_attachments_count">+%1$s</string>
    <!-- Share multiple attachments action label -->
    <string name="share_attachment_others">Share attachment <xliff:g example="IMAG_0001.jpg" id="file_name">%1$s</xliff:g> + <xliff:g example="5" id="item_count">%2$d</xliff:g> others</string>
    <!-- Share single attachment action label -->
    <string name="share_attachment">Share attachment %1$s</string>
    <!-- Share selected attachment action label -->
    <string name="share_attachment_selected">Share selected attachment</string>
    <!-- Content description for share all attachments button (accessibility) -->
    <string name="content_description_share_all">Share all attachments</string>
    <!-- Label for selected items count in multi-select lists -->
    <string name="items_selected"><xliff:g example="5" id="item_count">%1$s</xliff:g> item(s) selected</string>
    <!-- Error preparing attachments for sharing -->
    <string name="prepare_attachments_failure">Unable to prepare attachments for sharing, please try again later</string>

    <!--
      Attachment picker options
    -->
    <!-- Option for taking a photo with camera -->
    <string name="attachment_type_camera_image">Camera - photo</string>
    <!-- Option for recording a video with camera -->
    <string name="attachment_type_camera_video">Camera - video</string>
    <!-- Option for picking an image or video from gallery -->
    <string name="attachment_type_media">Media gallery</string>
    <!-- Option for picking a file from device storage -->
    <string name="attachment_type_file">File</string>

    <!--
      Attachment upload and error messages
    -->
    <!-- Error message for failed attachment upload -->
    <string name="attachment_upload_failure">Attachment upload failure</string>
    <!-- Error message for unsupported file type during attachment upload -->
    <string name="attachment_not_supported">Unable to upload attachment(s) due to invalid file type</string>
    <!-- Error message for oversized attachments -->
    <string name="attachment_too_large">Attachment(s) size greater than <xliff:g example="40" id="file_size">%1$d</xliff:g> MB</string>
    <!-- Error message for unreadable attachment -->
    <string name="attachment_read_error">Unable to read selected attachment.</string>
    <!-- Template for invalid attachments error message -->
    <string name="invalid_attachments_message_template">Unable to upload attachment(s) <xliff:g example="IMAGE_0001.jpg" id="filename">%1$s</xliff:g></string>
    <!-- Template for single invalid attachment upload error, message just states attachment name -->
    <string name="invalid_attachment_template"><xliff:g example="IMAGE_0001.jpg" id="filename">%1$s</xliff:g></string>

    <!--
      Warnings and validation errors
    -->
    <!-- Warning when only one conversation is allowed -->
    <string name="warning_thread_creation_forbidden">Chat configuration only allows usage of one conversation. Please use existing conversation to continue.</string>
    <!-- Warning to refresh thread list before creating new conversation -->
    <string name="warning_threads_refresh_required">Please refresh the conversation list before attempting to create a new conversation.</string>
    <!-- General failure warning for unhandled exceptions -->
    <string name="warning_general_failure">Unable to finish action, due to the unhandled exception. Please check log for details and/or report the issue.</string>
    <!-- Warning for missing required fields in pre-chat survey -->
    <string name="warning_missing_prechat_survey_response">Response to the pre-chat survey is missing required fields.</string>
    <!-- Failure due to missing required fields in pre-chat survey -->
    <string name="warning_missing_prechat_survey_response_failure">Unable to finish action. Response to the prechat survey is missing required fields.</string>
    <!-- Warning for invalid field values in forms -->
    <string name="warning_invalid_fields">Some of the fields contain invalid values</string>

    <!--
      Field validation errors
    -->
    <!-- Validation error for required field -->
    <string name="error_required_field">Required</string>
    <!-- Validation error for invalid email address -->
    <string name="error_email_validation">Invalid e-mail address</string>
    <!-- Validation error for invalid value -->
    <string name="error_value_validation">Invalid value</string>
    <!-- Validation error for invalid number -->
    <string name="error_invalid_number">Invalid number</string>
    <!-- Validation error for invalid integer -->
    <string name="error_invalid_integer">Invalid integer</string>
    <!-- Error for missing attachments in attachment message -->
    <string name="error_missing_attachments">Missing attachments in attachment message.</string>

    <!--
      Audio recording and playback
    -->
    <!-- Audio recording indicator shown while recording a voice message -->
    <string name="recording_audio_hint">Recording…</string>
    <!-- Permission rationale dialog for audio recording feature -->
    <string name="recording_audio_permission_rationale">Application requires write access to external storage and permission to record audio for this functionality</string>
    <!-- Dialog title when audio recording is unavailable due to permissions -->
    <string name="recording_audio_permission_denied_title">Audio message recording unavailable</string>
    <!-- Dialog message when audio recording is unavailable due to permissions -->
    <string name="recording_audio_permission_denied_body">Application is not able to record audio messages without permission to write access to external storage and permission to record audio</string>
    <!-- Label for previewing a recorded audio message -->
    <string name="recording_audio_preview_label">Audio message…</string>
    <!-- Prompt to review the recorded audio message before sending -->
    <string name="recording_audio_preview_message">Please review the recorded audio message</string>
    <!-- Error message when audio recording fails to start -->
    <string name="recording_audio_failed_to_start">Failed to start audio recording</string>
    <!-- General error message for failed audio recording -->
    <string name="recording_audio_failed">Audio recording has failed</string>
    <!-- Content description for the button to start audio recording -->
    <string name="record_audio_start_content_description">Record audio message</string>
    <!-- Content description for the button to delete a recorded audio message -->
    <string name="record_audio_delete_content_description">Delete recorded audio message</string>

    <!--
      Camera capture permissions
    -->
    <!-- Permission rationale dialog for camera capture feature -->
    <string name="camera_permission_rationale">Application requires camera permission to capture photos and videos</string>
    <!-- Dialog title when camera capture is unavailable due to permissions -->
    <string name="camera_permission_denied_title">Camera capture unavailable</string>
    <!-- Dialog message when camera capture is unavailable due to permissions -->
    <string name="camera_permission_denied_body">Application is not able to capture photos or videos without camera permission</string>

    <!--
        Audio capture permissions
    -->
    <!-- Content description for the button to stop audio recording -->
    <string name="record_audio_stop_content_description">Stop audio recording</string>
    <!-- Error message when temporary audio file cannot be deleted -->
    <string name="record_audio_failed_cleanup">Unable to delete temporary audio recording</string>

    <!--
      Pre-chat survey and custom fields
