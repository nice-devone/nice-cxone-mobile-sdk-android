    <string name="content_description_selection_frame">Selectable item</string>
    <!-- Content description for selected item frame (accessibility) -->
    <string name="content_description_selection_frame_selected">Item selected</string>
    <!-- Content description for play video button (accessibility) -->
    <string name="content_description_play_video">Play video</string>
    <!-- Content description for back button (accessibility) -->
    <string name="content_description_back_button">Back</string>
