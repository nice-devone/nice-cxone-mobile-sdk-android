    <string name="edit_custom_field_subtitle">Please fill out all fields and confirm the form.</string>
    <!-- Action to update the conversation/thread name -->
    <string name="update_thread_name">Update conversation name</string>
    <!-- Input hint for entering a conversation/thread name -->
    <string name="enter_thread_name">Enter conversation name</string>

