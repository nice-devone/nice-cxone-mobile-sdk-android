    <string name="content_description_url_link">URL link</string>

    <!--
      Plurals for media controls and notifications
    -->
    <!-- Plural for media control: seek back seconds -->
    <plurals name="seek_back">
        <item quantity="one">Seek back 1 second</item>
        <item quantity="other">Seek back <xliff:g example="5" id="seconds">%1$d</xliff:g> seconds</item>
    </plurals>
    <!-- Plural for media control: seek forward seconds -->
    <plurals name="seek_forward">
        <item quantity="one">Seek forward 1 second</item>
        <item quantity="other">Seek forward <xliff:g example="5" id="seconds">%1$d</xliff:g> seconds</item>
    </plurals>

    <!--
      Notification and permission dialogs
    -->
    <!-- Title for permission request dialog -->
    <string name="permission_requested">Permission requested</string>
    <!-- Rationale for notification permission request -->
    <string name="notifications_rationale">The application would like to send you notifications if your agent sends you messages when the application is suspended.</string>
    <!-- Title for unavailable notifications dialog -->
    <string name="no_notifications_title">Notifications unavailable</string>
    <!-- Message for unavailable notifications dialog -->
    <string name="no_notifications_message">The application can not send you notifications if your agent sends you messages when the application is suspended.</string>
    <!-- Notification channel name for chat notifications -->
    <string name="notification_channel_title">Chat notifications</string>
    <!-- Default notification sender name for chat notifications -->
    <string name="notification_person_name">Agent</string>
    <!-- Plural for notification: attachment count -->
    <plurals name="notification_attachment">
        <item quantity="one"><xliff:g example="1" id="attachments">%1$d</xliff:g> attachment</item>
        <item quantity="other"><xliff:g example="5" id="attachments">%1$d</xliff:g> attachments</item>
    </plurals>
    <!-- Fallback for unknown file extension in document preview -->
    <string name="fallback_document_initial_extension">N/A</string>

    <!--
      Chat permissions for Android manifest
    -->
    <!-- Description for internal permission of the chat -->
    <string name="desc_chat_permission">Allows the application to open the chat view.</string>
    <!-- Permission label for chat view in Android manifest -->
    <string name="label_chat_permission">Open chat view</string>

    <!--
      Agent and unsupported message status
    -->
    <!-- Label for no agent assigned to conversation -->
    <string name="no_agent_assigned">No agent</string>
    <!-- Status for undeliverable/unsupported messages -->
    <string name="unsupported_message_status">Message cannot be displayed</string>
    <!-- Message for unsupported message content in chat -->
    <string name="unsupported_message">This message could not be displayed in the chat as it uses a format that is currently unsupported.</string>

