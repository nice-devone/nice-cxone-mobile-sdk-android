    <string name="quick_reply_disable_message">These options are no longer visible because you\'ve already responded or the message is no longer the most recent.</string>
    <!-- Quick Reply - The label for state when options from message are unavailable -->
    <string name="quick_reply_options_unavailable">Options unavailable</string>
    <!-- List picker open prompt -->
    <string name="list_picker_open_message">Press to open</string>
    <!-- Label indicating an option was selected (e.g. quick reply, list picker) -->
    <string name="option_selected">Option selected</string>
    <!-- Prompt for single-choice quick reply -->
    <string name="select_option_below">Select one option below</string>

